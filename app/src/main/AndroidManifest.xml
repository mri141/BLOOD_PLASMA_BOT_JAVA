<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jetlight.rashed.BloodPlasmaBot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Blood Plasma Bot"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.LoginActivity" />
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.DonorActivity" />
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.ProfileActivity" />
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.SearchActivity" />
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.StatusActivity" />
        <activity android:name="com.jetlight.rashed.BloodPlasmaBot.AccountActivity"></activity>
    </application>
</manifest>